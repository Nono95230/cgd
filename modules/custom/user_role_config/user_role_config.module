<?php

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function user_role_config_help($route_name, RouteMatchInterface $route_match){
  switch ($route_name) {
    case 'help.page.user_role_config':
      $output = '';
      $output .= '<h3>'.t('About').'</h3>';
      $output .= '<p>'.t('Please send help').'</p>';

      return $output;
  }
}


